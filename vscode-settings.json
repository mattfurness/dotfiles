    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "p",
            ],
            "after": [
                "p",
                "g",
                "v",
                "y"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        // Go replace inner word
        {
            "before": [
                "g",
                "r",
                "i",
                "w"
            ],
            "after": [
                "v",
                "i",
                "w",
                "p"
            ]
        },
        // Go replace inner "
        {
            "before": [
                "g",
                "r",
                "i",
                "\""
            ],
            "after": [
                "v",
                "i",
                "\"",
                "p"
            ]
        },
        // Go replace inner '
        {
            "before": [
                "g",
                "r",
                "i",
                "'"
            ],
            "after": [
                "v",
                "i",
                "'",
                "p"
            ]
        },
        // Go replace inner ()
        {
            "before": [
                "g",
                "r",
                "i",
                "("
            ],
            "after": [
                "v",
                "i",
                "(",
                "p"
            ]
        },
        {
            "before": [
                "g",
                "r",
                "i",
                ")"
            ],
            "after": [
                "v",
                "i",
                ")",
                "p"
            ]
        },
        // Go replace inner []
        {
            "before": [
                "g",
                "r",
                "i",
                "["
            ],
            "after": [
                "v",
                "i",
                "[",
                "p"
            ]
        },
        {
            "before": [
                "g",
                "r",
                "i",
                "]"
            ],
            "after": [
                "v",
                "i",
                "]",
                "p"
            ]
        },
        // Yank to end of line
        {
            "before": [
                "Y"
            ],
            "after": [
                "y",
                "$"
            ]
        }
    ],
    "FSharp.fsacRuntime": "netcore",
    "fantomas.preserveEOL": true,
    "fantomas.indent": 2,
    "fantomas.pageWidth": 120,
    "editor.formatOnSaveTimeout": 1500,
    "files.autoSaveDelay": 3000
}
