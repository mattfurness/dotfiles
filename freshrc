# freshshell.com
#
# Examples:
#
#   fresh twe4ked/dotfiles shell/functions/\*
#   fresh jasoncodes/dotfiles shell/aliases/rails.sh
#   fresh jasoncodes/dotfiles config/tmux.conf --file
#
# See http://freshshell.com/readme for documentation.

fresh freshshell/fresh bin/fresh --bin

fresh freshshell/fresh contrib/source-build.sh --file=~/.bash_profile
fresh freshshell/fresh contrib/source-build.sh --file=~/.bashrc

fresh twe4ked/dotfiles config/ackrc --file
fresh jasoncodes/dotfiles config/gemrc --file
fresh Brewfile --file
fresh inputrc --file
fresh ruby-version --file

fresh freshshell/fresh contrib/completion/fresh-completion.bash
fresh twe4ked/dotfiles shell/aliases/git.sh
fresh jasoncodes/dotfiles shell/aliases/\*
fresh foobacca/dotfiles shell/aliases-dir
fresh shell/aliases/\*
fresh shell/completion/\*
fresh shell/path/\*
fresh shell/prompt/\*
fresh shell/env/\*
fresh rupa/z z.sh

fresh 'vim/plugins.vim' --file=~/.vimrc --marker='"'
fresh 'vim/global.vim' --file=~/.vimrc --marker='"'
fresh 'vim/grep.vim' --file=~/.vimrc --marker='"'
